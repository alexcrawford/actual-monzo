{
  "name": "actual-monzo",
  "version": "0.1.0",
  "description": "CLI tool to import Monzo transactions into Actual Budget",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "actual-monzo": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/ --ext .ts,.tsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx --fix",
    "format": "prettier --write src/ tests/",
    "type-check": "tsc --noEmit",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build && pnpm test:run"
  },
  "dependencies": {
    "@actual-app/api": "^25.9.0",
    "axios": "^1.12.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "conf": "^12.0.0",
    "date-fns": "^2.30.0",
    "inquirer": "^9.2.12",
    "js-yaml": "^4.1.0",
    "open": "^10.2.0",
    "ora": "^7.0.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@inquirer/testing": "^2.1.50",
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.52.0",
    "memfs": "^4.48.1",
    "nock": "^14.0.10",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.17.1",
  "keywords": [
    "monzo",
    "actual-budget",
    "cli",
    "finance",
    "typescript",
    "banking",
    "transactions",
    "sync",
    "import"
  ],
  "author": "Alex Crawford <alex@kick.digital>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexcrawford/actual-monzo.git"
  },
  "bugs": {
    "url": "https://github.com/alexcrawford/actual-monzo/issues"
  },
  "homepage": "https://github.com/alexcrawford/actual-monzo#readme"
}